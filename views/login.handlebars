<div class="row col-md-9 col-lg-9 ">
  <div class="col-md-9 col-lg-9">
    <h2>Login</h2>

    <form class="form login-form" autocomplete="off">
      <div class="form-group">
        <label for="email-login">email:</label>
        <input class="form-input" type="text" id="email-login" />
      </div>
      <div class="form-group">
        <label for="password-login">password:</label>
        <input class="form-input" type="password" id="password-login" />
      </div>
      <div class="form-group button-group">
        <button class="btn pst-btn" type="submit">Login</button>
      </div>
      <div>
        <p> <a href="/signup"> SignUp instead </a> </p>
      </div>
    </form>
  </div>
</div>

<script src="./js/login.js"></script>
